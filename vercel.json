{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_URL": "https://travelstreamz-app.vercel.app",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://travelstreamz-app.vercel.app/api"
    }
  }
}
